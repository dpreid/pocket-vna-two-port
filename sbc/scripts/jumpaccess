#!/bin/bash
mkdir -p ../autogenerated 
cd ../autogenerated

# example: jumpaccess https://shell-access2.practable.io pvna 32 34 #makes files for pvna32, pvna33, and pvna34

for i in $(seq $3 $4)
do
    num=$(printf "%02d" $i)
	name="${2}${num}"
	echo "${1}/shell/${name}" > "jump.access.${name}"  
done
